{
  "name": "test-history-agui",
  "version": "0.1.0",
  "description": "Open-source CopilotKit + LangGraph chat history persistence. Restore thread history on page refresh.",
  "license": "MIT",
  "author": "Benjamin Alaka <github.com/alakaybenjamin>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alakaybenjamin/copilotkit-langgraph-history-extend.git"
  },
  "homepage": "https://github.com/alakaybenjamin/copilotkit-langgraph-history-extend#readme",
  "bugs": {
    "url": "https://github.com/alakaybenjamin/copilotkit-langgraph-history-extend/issues"
  },
  "keywords": [
    "copilotkit",
    "langgraph",
    "langchain",
    "ai-agent",
    "chat-history",
    "thread-persistence",
    "nextjs",
    "react",
    "typescript",
    "open-source",
    "chat",
    "history",
    "hydration"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run typecheck && pnpm run build",
    "clean": "rm -rf dist",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "@ag-ui/core": ">=0.0.40",
    "@copilotkit/runtime": ">=1.50.0",
    "@copilotkitnext/runtime": ">=0.0.30",
    "@langchain/langgraph-sdk": ">=1.0.0",
    "rxjs": ">=7.0.0"
  },
  "peerDependenciesMeta": {
    "@ag-ui/core": {
      "optional": false
    },
    "@copilotkit/runtime": {
      "optional": false
    },
    "@copilotkitnext/runtime": {
      "optional": false
    },
    "@langchain/langgraph-sdk": {
      "optional": false
    },
    "rxjs": {
      "optional": false
    }
  },
  "devDependencies": {
    "@ag-ui/core": "^0.0.42",
    "@copilotkit/runtime": "^1.50.1",
    "@copilotkitnext/runtime": "^0.0.33",
    "@langchain/langgraph-sdk": "^1.3.1",
    "@types/node": "^20.17.12",
    "eslint": "^9.17.0",
    "rxjs": "7.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
